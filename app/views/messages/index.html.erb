<div class="col-md-8 col-md-offset-2">
  <h1 class="new-msg-header">All Messages</h1>

  <% @messages.each do |message| %>
      <div class="col-md-10 col-md-offset-1">
        <div class="message">
          <h2 class="message-title">
            <%= message.title %>
          </h2>
          <%= link_to 'Join Convo', message_path(message), class: "btn-custom" %>
        </div>
      </div>

  <% end %>

</div>
